<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Oracle Corporation//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>yuku.pdb2yes.servlet.ConvertPdbToYes</servlet-name>
        <servlet-class>yuku.pdb2yes.servlet.ConvertPdb</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>yuku.pdb2yes.servlet.ConvertPdbToYes</servlet-name>
        <url-pattern>/yuku.pdb2yes.servlet.ConvertPdb</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>yuku.pdb2yes.servlet.DownloadBlob</servlet-name>
        <servlet-class>yuku.pdb2yes.servlet.DownloadBlob</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>yuku.pdb2yes.servlet.DownloadBlob</servlet-name>
        <url-pattern>/yuku.pdb2yes.servlet.DownloadBlob</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
